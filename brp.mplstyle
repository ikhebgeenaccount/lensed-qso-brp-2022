# Axes
axes.titlesize : 20
axes.labelsize : 20

# Graphs
lines.linewidth : 1
lines.markersize : 8
xtick.labelsize : 20
ytick.labelsize : 20

errorbar.capsize : 5

# Figure
figure.figsize : 10, 6
figure.subplot.left: 0.08
figure.subplot.right: 0.98
figure.subplot.bottom: 0.12
figure.subplot.top: 0.95

# Legend
legend.fontsize : 20

# Savefig
savefig.format : pdf

# Font
# text.Text properties
font.size : 20
